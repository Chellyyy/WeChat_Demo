<!--logs.wxml-->
<view>
  <swiper indicator-dots="{{indicatorDots}}"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}" wx:key="{{imgUrls}}">
      <swiper-item >
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
  <view class='logs-container'>
    <view class='logs-author-date'>
      <image class='post-author' src='/images/avatar/1.png'></image>
      <text class='logs-date'>July 17 2018</text>
    </view>
    <text class='logs-title'>文章标题</text>
    <image class='logs-image' src='/images/post/crab.png'></image>
    <text class='logs-content'>文章简介文章简介文章简介</text>
    <view class='logs-data'>
      <image class='logs-data-img' src='../../images/icon/chat.png'></image>
      <text class='logs-data-coll'>92</text>
      <image class='logs-data-img' src='../../images/icon/view.png'></image>
      <text class='logs-data-view'>65</text>
    </view>
  </view>
</view>
