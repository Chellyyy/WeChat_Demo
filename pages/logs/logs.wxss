.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
swiper {
  width: 100%;
  height: 600rpx;
}
swiper image{
  width: 100%; 
  height: 600rpx;
}
.logs-container{
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  padding: 5px;
}
.logs-author-data{
  margin: 10rpx 0 20rpx 10rpx;

}
.post-author{
  width: 60rpx;
  height: 60rpx;
  vertical-align: center;
}
.logs-data{
  margin-left: 20rpx;
  vertical-align: center;
  height: 60rpx;
}
.logs-data{
  margin-left: 20rpx;
  vertical-align: center;
  margin-bottom: 5px;
  font-size: 26rpx;
}
.logs-title{
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
  margin-bottom: 10px;
  margin-left: 10px;

}
